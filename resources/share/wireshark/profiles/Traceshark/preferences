####### User Interface: Columns ########

# Packet list column format
# Each pair of strings consists of a column title and its format
gui.column.format:
	"No.", "%m",
	"Time", "%t",
    "Machine", "%Cus:frame.machine_id:0:R",
	"Source", "%s",
	"Destination", "%d",
	"Protocol", "%p",
	"Length", "%L",
    "PID", "%Cus:frame.pid:0:R",
	"Info", "%i"